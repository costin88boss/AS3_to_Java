		public static function isSolid(id:int):Boolean{
			return !isClimbable(id) &&
			(((9 <= id && id <= 97) ||     //9 <= id <= 97
			(122 <= id && id <= 217) ||    //122 <= id <= 217
			(id >= 1001 && id <= 1499)) && //1001 <= id <= 1499 
			id != 83 && id != 77);
		}
